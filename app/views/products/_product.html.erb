<div id="<%= dom_id product %>">
  <p>
    <strong>Title:</strong>
    <%= product.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>{5-2}:</strong>
    <%= product.{5-2} %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= product.image_url %>
  </p>

</div>
